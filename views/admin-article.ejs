<script>
  $.get("/admins/getArticleCount",function(result){

    var amount=parseInt(result[0]['count(*)']);

    console.log('amount',amount);
    pageamount=Math.ceil(amount/6);
    console.log(pageamount);
    $("#length").text("共 "+amount+ "记录");
    $('#list').html('');
    for (var i=0;i<pageamount;i++){
      var j=i+1;
      $("#list").append("<li><a href='javascript:void(0);'>" + j + "</a></li>")
    }
    $("#list li:first").addClass("active");
    $("#list li").click(function(){
      var pages=$(this).index();
      console.log(pages);
      getPage(pages);
      $(this).addClass("active").siblings().removeClass("active");
    })
  })

</script>

<script>
  var $tableList = $(".table-list");
  //得到模板函数
  getPage(0);
  function getPage(pages){
    //将现在页面清除
    $(".table-list").html("");
    console.log("这是pages"+pages);
    $.ajax({
      "url":"/admins/getAllArticle?page="+pages,
      "type":"get",
      "dataType":"json",
      "success":function(data){
        var lis=new Array();
        for(var i in data){
          var d=data[i];
          var table=$("<table width='100%'border='1'>");
          var tr=$("<tr>");;
          var article_id=$("<td style='word-break:break-all' width='5%' align='center'>", {class: "TABLE-id"}).text(d.article_id);
          var titile=$("<td style='word-break:break-all' width='20%' align='center'>", {class: "TABLE-title"}).text(d.title);
          var content=$("<td style='word-break:break-all' width='55%'>", {class: "TABLE-title"}).text(d.contents);
          var time = $("<td style='word-break:break-all' width='10%'align='center'>", {class: "TABLE-desc"}).text(d.add_time.toLocaleString());
          var userid=$("<td style='word-break:break-all' width='5%' align='center'>", {class: "TABLE-status"}).text(d.user_id);
          var likeamount=$("<td style='word-break:break-all' width='5%' align='center'>", {class: "TABLE-receiver"}).text(d.like_amount);
          table.append(tr);
          tr.append(article_id);
          tr.append(titile);
          tr.append(content);
          tr.append(time);
